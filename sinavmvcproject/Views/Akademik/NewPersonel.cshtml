@model sinavmvcproject.Models.Entity.akademikp
@{
    ViewBag.Title = "NewPersonel";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form method="post" class="form-group">
    <div>
        <label>Personel Adı</label>
        <input type="text" class="form-control" name="isim" />
    </div>
    <div style="margin-top:10px">
        <label>Personel Soyadı</label>
        <input type="text" class="form-control" name="soyisim" />
    </div>
    <div style="margin-top:10px">
        <label>Personel Kullanıcı Adı</label>
        <input type="text" class="form-control" name="kadı" />
    </div>
    <div style="margin-top:10px">
        <label>Personel Şifre</label>
        <input type="text" class="form-control" name="sifre" />
    </div>
    <div style="margin-top:10px">
        <button class="btn btn-primary">Öğrenci Ekle</button>
    </div>
</form>

